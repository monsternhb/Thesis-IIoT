<!-- header -->
<header class="header">
      <nav class="nav" >
        <img
          src="https://cdn.haitrieu.com/wp-content/uploads/2021/09/Logo-DH-Bach-Khoa-HCMUT.png"
          alt="BK logo"
          class="nav__logo"
          id="logo"
          designer="Jonas"
          data-version-number="3.0"
        />
        <ul class="nav__links">
          <li class="nav__item ">
            <a class="nav__link home" >Home</a>
          </li>
          <li class="nav__item">
            <a class="nav__link history" >History</a>
          </li>
          <li class="nav__item">
            <a class="nav__link alarm" >Alarm</a>
          </li>
          <li class="nav__item">
            <a class="nav__link device" >Device</a>
          </li>
          <li class="nav__item">
            <a class="nav__link nav__link--btn btn--show-modal btn--nav register" 
              >Register</a>
          </li>
          <li class="nav__item">
            <button class="btn--log-out btn--nav" id={{role}}>Log out</button>
          </li>
        </ul>
      </nav>
      <hr class="gach-ngang" width="90%" align="center" />
</header>

{{#if devIp}}
  <div class="alert alert-light noti__dev" role="alert">
    {{devIp}}
  </div>
{{else}}
  <div class="alert alert-light noti__dev" role="alert">
    Click here to select your device!!
  </div>
{{/if}}

<form method="GET" action="/home/logout" name = 'form__log-out'></form>

<script>
  document.addEventListener('DOMContentLoaded',function(){
      const btnLogOut = document.querySelector('.btn--log-out');
      const formLogOut=document.forms['form__log-out'];
      const role = btnLogOut.id;
      console.log(`Login by a ${role}`);

      //edit link 
      const editHome = document.querySelector('.home');
      const editHistory = document.querySelector('.history');
      const editAlarm = document.querySelector('.alarm');
      const editDevice = document.querySelector('.device');
      const editRegister = document.querySelector('.register');

      editHome.setAttribute('href',`/${role}/home`);
      editHistory.setAttribute('href',`/${role}/history`);
      editAlarm.setAttribute('href',`/${role}/alarm`);
      editDevice.setAttribute('href',`/${role}/device`);
      editRegister.setAttribute('href',`/${role}/register`);

      if (role === 'manager' || role === 'operator' || role === 'viewer'){
        editHome.setAttribute('href',`/register/home`);
        editHistory.setAttribute('href',`/register/history`);
        editAlarm.setAttribute('href',`/register/alarm`);
        editDevice.setAttribute('href',`/register/device`);
        editRegister.setAttribute('href',`/register/register`);
      } 

    if (role === 'viewer') editRegister.classList.add('hidden');
      btnLogOut.addEventListener('click',function(){
        formLogOut.submit();
      })
  })
</script>